===\n\n//Breakpoints\n$tablet: 680px;\n$largeScreens: 1024px;\n$extraLargeScreens: 1400px; \n\n\n// Default Page Width\n$pageWidth: 1400px;\n\n\n// FONT STYLES =====\n$base-font-family: 'Public Sans', sans-serif;;\n$base-font-size: 18px;\n\n","scss/_header.scss":".top-container {\n  position: relative;\n  z-index: 2;\n  background-color: $white;\n}\nheader {\n  max-width: 1225px;\n  margin: auto;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  position: relative;\n  z-index: 2;\n  \n  .logo {\n    max-width: 100px;\n    max-height: 20px;\n  }\n\n  nav {\n    text-align: center;\n      ul {\n        font-size: 12px;\n        list-style: none;\n        margin: 0;\n        padding: 0;\n        display: flex;\n\n        li {\n          margin: 0 5px;\n        \n          a {\n            color: $grayishBlue;\n            width: 55px;\n            padding: 30px 0;\n            display: block;\n            transition: color .5s ease-in-out;\n            text-decoration: none;\n    \n            &:hover {\n              color: $darkblue;\n              }\n          }\n\n          .border {        \n            content: \"\";\n            height: 4px;\n            display: block;\n            background: $white;\n          }\n\n          a:hover + .border {\n           background: linear-gradient(90deg, $limeGreen 0%, $brightCyan 100%);\n          }\n          \n        }\n\n       \n      }\n  }\n}","scss/_elements.scss":".row {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  grid-gap: 30px;\n}\n\n\n//  HERO ======\n.hero-outer-container {\n  position: relative;\n}\n.hero-container {\n  background-color: $veryLightGray;\n  height: 80vh;\n  position: relative;\n  overflow: hidden;\n \n  &__inner {\n    display: grid;\n    grid-template-columns: 50% 50%;\n    align-items: center;\n    max-width: 1225px;\n    margin: 0 auto;\n    height: 100%;\n    position: relative;\n\n   \n\n    .hero {\n      width: 75%;\n\n      &__heading {\n         h1 {\n           margin: 0;\n           font-size: 50px;\n         }\n      }\n    \n      &__text {\n        padding: 10px 0 20px;\n      }\n    }\n  }\n\n  .hero-img {\n    position: absolute;\n    top: -30%;\n    right: -40%;\n    width: 100%;\n  }\n}\n.hero-overlay {\n  position: absolute;\n  top: 0;\n  overflow: hidden;\n  width: 100%;\n  height: 100%;\n  .hero-img-one {\n    position: absolute;\n    top: -7%;\n    right: -9%;\n    z-index: 1;\n    min-width: 300px;\n    max-width: 750px;\n  }\n}\n\nmain {\n  padding: 60px 20px;\n  background-color: $lightGrayishBlue;\n\n  .content {\n    max-width: 1225px;\n    margin: 0 auto;\n  }\n  .finances-container {\n\n    .intro {\n      max-width: 650px;\n      padding-bottom: 60px\n    }\n  }\n  .finances {\n\n    &__title {\n      margin: 40px 0 30px;\n    }\n\n    &__content {\n\n    }\n\n    &__img {\n      max-width: 70px;\n    }\n  }\n}\n\n\n.latest-articles {\n  padding: 80px 20px;\n  background-color: $veryLightGray;\n\n  &__inner {\n    max-width: 1225px;\n    margin: 0 auto;\n    .article {\n      background-color: $white;\n      \n      &__content {\n        padding: 30px 20px 20px;\n\n        .title {\n          margin: 0.5em 0;\n\n          a {\n            color: $darkblue;\n            transition: color .2s ease-in-out;\n            text-decoration: none;\n\n            &:hover {\n              color: $limeGreen;\n            }\n          }\n        }\n        .excerpt {\n          margin: 0;\n          font-size: 16px;\n        }\n      }\n\n      &__img {\n        object-fit: cover;\n        height: 200px;\n      }\n    }\n  }\n}\n\n\n// Buttons\n\nbutton {\n  color: $white;\n  border: 0;\n  width: 160px;\n\n  &:focus {\n    outline: none;\n  }\n}\n\n.btn {\n  @include borderRadius(60px); \n  padding: 15px 30px;\n  font-weight: 400;\n  background: $limeGreen;\nbackground: linear-gradient(90deg, $limeGreen 0%, $brightCyan 100%);\n\n&:hover {\n  opacity: 0.8;\n}\n}","scss/_mixins.scss":"@mixin borderRadius($radius) {\n  border-radius: $radius;\n}","scss/_footer.scss":".footer {\n  background-color: $darkblue;\n\n  &__inner {\n    max-width: 1225px;\n    margin: 0 auto;\n    padding: 40px 0;\n\n    .row {\n      grid-template-columns: 1fr 2fr 1fr;\n\n      .footer-col-1 {\n      \n        .social-icons {\n          display: flex;\n          padding-top: 50px;\n\n          img {\n            max-width: 20px;\n            margin: 0 5px; \n          }\n        }\n      }\n      .footer-col-2 {\n  \n        li {\n          font-size: 14px;\n          padding: 10px 0 0;\n        }\n      }\n\n      .footer-col-3 {\n        justify-self: end;\n        display: flex;\n        flex-direction: column;\n        align-items: flex-end;\n\n        small {\n          display:block;\n          padding: 20px 0;\n        }\n      }\n    }\n    svg {\n      max-width: 150px;\n    }\n        .footer-svg {\n          fill: $white;\n        }\n    }\n\n  ul {\n    color: $veryLightGray;\n    list-style: none;\n    margin: 0;\n    padding: 0;\n    display: flex;\n    flex-wrap: wrap;\n    li {\n      width: 50%;\n    }\n  }\n\n  .attribution {\n     padding: 20px;\n  }\n}"},"lineCount":null}},"error":null,"hash":"d31fc4342d24a26cf71b971cd8c03fc0","cacheData":{"env":{}}}